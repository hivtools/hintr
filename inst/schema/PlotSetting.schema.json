{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "label": {
            "type": "string"
        },
        "options": {
            "type": "array",
            "items": { "$ref": "PlotSettingOption.schema.json" }
        },
        "value": {
            "type": "string"
        },
        "hidden": {
            "type": "boolean"
        }
    },
    "additionalProperties": false,
    "required": [ "id", "label", "options" ]
}
