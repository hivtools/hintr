{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data_type": {
      "type": "string",
      "enum": [ "spectrum", "calibrated", "raw", "calibration_ratio" ]
    },
    "spectrum_region_code": {
      "type": "string"
    },
    "spectrum_region_name": {
      "type": "string"
    },
    "sex": {
      "type": "string"
    },
    "age_group": {
      "type": "string"
    },
    "calendar_quarter": {
      "type": "string"
    },
    "indicator": {
      "type": "string"
    },
    "mean": {
      "type": [ "number", "null" ]
    }
  },
  "additionalProperties": true,
  "required": [ "data_type", "spectrum_region_code", "spectrum_region_name",
    "sex", "age_group", "calendar_quarter", "indicator", "mean" ]
}
