{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "definitions": {
        "tableEffect": {
            "type": "object",
            "properties": {
                "row": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "column": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "additionalProperties": false,
            "required": [ "row", "column" ]
        }
    },
    "type": "object",
    "properties": {
        "setFilters": {
            "type": "array",
            "items": { "$ref": "FilterRef.schema.json" }
        },
        "setMultiple": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "setFilterValues": {
            "type": "object",
            "properties": {},
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            }
        },
        "setHidden": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "plotEffect": {
          "oneOf": [
              { "$ref": "#/definitions/tableEffect" }
          ]
        }
    },
    "additionalProperties": false
}
