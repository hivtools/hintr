{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "column_id": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "options": {
      "type": [ "array", "null" ],
      "items": { "$ref": "FilterOption.schema.json" }
    },
    "use_shape_regions": {
      "type": [ "boolean", "null" ]
    },
    "use_shape_area_level": {
      "type": [ "boolean", "null" ]
    }
  },
  "additionalProperties": false,
  "required": [ "id", "column_id", "label", "options" ]
}
