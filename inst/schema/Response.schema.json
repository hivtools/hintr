{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "status": {
      "enum": [ "success", "failure" ]
    },
    "data": {},
    "errors": {
      "type": "array",
      "items": { "$ref": "Error.schema.json" }
    }
  },
  "additionalProperties": false,
  "required": ["status", "errors", "data" ]
}
