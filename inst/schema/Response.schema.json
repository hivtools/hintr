{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "status": {
      "enum": [ "success", "failure" ]
    },
    "data": {
      "type": "object"
    },
    "errors": {
      "type": "array",
      "items": { "$ref": "Error.schema.json" }
    },
    "version": {
      "type": "object",
      "properties": {
        "hintr": { "type": "string" },
        "naomi": { "type": "string" },
        "rrq": { "type": "string" }
      },
      "additionalProperties": true,
      "required": [ "hintr", "naomi", "rrq" ]
    }
  },
  "additionalProperties": false,
  "required": ["status", "errors", "data" ]
}
