{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "area_id": {
      "type": "string"
    },
    "area_name": {
      "type": "string"
    },
    "area_level_label": {
      "type": "string"
    },
    "year": {
      "type": "number"
    },
    "time_step": {
      "type": "string"
    },
    "plot": {
      "type": [ "string", "null" ]
    },
    "value": {
      "type": [ "number", "null" ]
    },
    "area_level": {
      "type": "number"
    },
     "age_group": {
      "type": "string"
    },
     "time_period": {
      "type": "string"
    }
  },
  "additionalProperties": true,
  "required": [ "area_id", "area_name", "area_level_label", "year", "time_step",
    "plot", "value" ],
  "oneOf": [
    { "required": [ "area_level", "age_group"] },
    { "required":  [ "time_period" ] }
  ]
}
